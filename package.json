{
  "name": "recompact",
  "version": "1.0.2",
  "description": "A set of React high order components based on Rx.",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:neoziro/recompact.git",
    "type": "git"
  },
  "scripts": {
    "build:doc": "babel-node ./scripts/build-doc",
    "build:lib": "rm -rf lib/ && babel --ignore src/__tests__ src -d lib && cp package.json lib/package.json && rm -rf lib/__benchmarks__",
    "test": "eslint . && jest -i",
    "examples": "webpack-dev-server"
  },
  "author": "Greg Berg√© <neoziro@gmail.com>",
  "license": "MIT",
  "jest": {
    "testRegex": "\\.test\\.js$",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "keywords": [
    "composition",
    "utilities",
    "toolkit",
    "microcomponentization",
    "components",
    "higher-order",
    "react",
    "recompose"
  ],
  "peerDependencies": {
    "react": "^15.3.2",
    "rxjs": "^5.0.0-rc.4"
  },
  "dependencies": {
    "change-emitter": "^0.1.2",
    "fbjs": "^0.8.8",
    "hoist-non-react-statics": "^1.2.0",
    "object.omit": "^2.0.1",
    "object.pick": "^1.2.0",
    "symbol-observable": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.0",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "benchmark": "^2.1.2",
    "docdown": "neoziro/docdown#multiple-files-support",
    "enzyme": "^2.7.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "fs-extra": "^1.0.0",
    "html-webpack-plugin": "^2.26.0",
    "jest": "^18.0.0",
    "jsdom": "^9.9.1",
    "microtime": "^2.1.2",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "recompose": "^0.21.2",
    "rxjs": "^5.0.2",
    "sinon": "^1.17.6",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
