{
  "name": "recompact",
  "version": "2.3.1",
  "description": "A set of React high order components based on Rx.",
  "main": "index.js",
  "repository": {
    "url": "git@github.com:neoziro/recompact.git",
    "type": "git"
  },
  "scripts": {
    "build": "yarn build:doc && yarn build:lib",
    "build:doc": "NODE_ENV=production babel-node ./scripts/build-doc",
    "build:lib": "rm -rf lib/ && NODE_ENV=production babel --ignore src/__tests__ src -d lib && cp package.json lib/package.json && rm -rf lib/__benchmarks__",
    "test": "eslint . && jest -i && codecov",
    "examples": "webpack-dev-server"
  },
  "author": "Greg Berg√© <neoziro@gmail.com>",
  "license": "MIT",
  "jest": {
    "testRegex": "\\.test\\.js$",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true
  },
  "keywords": [
    "composition",
    "utilities",
    "toolkit",
    "microcomponentization",
    "components",
    "higher-order",
    "react",
    "recompose"
  ],
  "peerDependencies": {
    "react": "^15.5.4"
  },
  "dependencies": {
    "fbjs": "^0.8.12",
    "hoist-non-react-statics": "^1.2.0",
    "object.omit": "^2.0.1",
    "object.pick": "^1.2.0",
    "symbol-observable": "^1.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-loader": "^7.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "benchmark": "^2.1.4",
    "codecov": "^2.1.0",
    "create-react-class": "^15.5.2",
    "docdown": "neoziro/docdown#multiple-files-support",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "fs-extra": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jsdom": "^9.12.0",
    "microtime": "^2.1.3",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "reassemble": "^0.5.6",
    "recompose": "^0.23.1",
    "rxjs": "^5.3.0",
    "sinon": "^2.1.0",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.3"
  }
}
